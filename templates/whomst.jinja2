<style>
  #whomst {
  margin-left: auto;
  margin-right: auto;
  text-align: left;
  }

  table, th, td {
  border: 1px solid black;
  }
</style>

<table id="whomst">
  <tr>
    <td>Display name</td>
    <td>Connect code</td>
    <td>IP-address</td>
    <td>Area</td>
    <td>Timestamp</td>
  </tr>
  {% for whomst in whomsts %}
    <tr>
      <td>
	{{ whomst['display_name'] }}
      </td>
      <td>
	<a href="/whomst/search/connect_code/{{ whomst['connect_code'] | replace("#", "_") }}">
	  {{ whomst['connect_code'] }}
	</a>
      </td>
      <td>
	<a href="/whomst/search/ip_address/{{ whomst['ip_address'] }}/24">
	  {{ whomst['ip_address'] }}
	</a>
      </td>
      <td>
	{{ whomst['region'] }}
      </td>
      <td>
	{{ whomst['time_added'] }}
      </td>
    </tr>
  {% endfor %}
</table>
